@prefix : <http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/tom/ontologies/2021/0/modsem_beer> .

<http://www.semanticweb.org/tom/ontologies/2021/0/modsem_beer> rdf:type owl:Ontology ;
                                                                rdfs:label "Beer Ontology" .

#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#blended_with
:blended_with rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:domain :beer ;
              rdfs:range :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#created_by
:created_by rdf:type owl:ObjectProperty ;
            rdfs:domain :beer ;
            rdfs:range :beer_producer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#creates
:creates rdf:type owl:ObjectProperty ;
         rdfs:domain :beer_producer ;
         rdfs:range :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_beer_feature
:has_beer_feature rdf:type owl:ObjectProperty ;
                  rdfs:domain :beer ;
                  rdfs:range :beer_features .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_container
:has_container rdf:type owl:ObjectProperty ;
               rdfs:domain :beer ;
               rdfs:range :container .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_era
:has_era rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :has_beer_feature ;
         rdf:type owl:FunctionalProperty ;
         rdfs:domain :beer ;
         rdfs:range :era .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_fermentation
:has_fermentation rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :has_beer_feature ;
                  rdfs:domain :beer ;
                  rdfs:range :fermentation .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_flavour
:has_flavour rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :has_beer_feature ;
             rdfs:domain :beer ;
             rdfs:range :flavour .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_ingredient
:has_ingredient rdf:type owl:ObjectProperty ;
                rdfs:domain :beer ;
                rdfs:range :ingredient .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_style
:has_style rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdf:type owl:FunctionalProperty ;
           rdfs:domain :beer ;
           rdfs:range :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_substyle
:has_substyle rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :has_beer_feature ;
              owl:inverseOf :is_substyle_of ;
              rdfs:domain :beer ;
              rdfs:range :substyle .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#is_substyle_of
:is_substyle_of rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :has_beer_feature .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#located_in
:located_in rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:range :region ;
            owl:propertyChainAxiom ( :created_by
                                     :located_in
                                   ) ,
                                   ( :creates
                                     :located_in
                                   ) .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_abv
:has_abv rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :beer ;
         rdfs:range xsd:float .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_ibu
:has_ibu rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :beer ;
         rdfs:range xsd:nonNegativeInteger .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_srm
:has_srm rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :beer ;
         rdfs:range xsd:positiveInteger .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#has_year
:has_year rdf:type owl:DatatypeProperty ;
          rdfs:domain :beer ;
          rdfs:range xsd:nonNegativeInteger .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#produces_hl
:produces_hl rdf:type owl:DatatypeProperty ;
             rdfs:domain :beer_producer ;
             rdfs:range xsd:positiveInteger .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#abv_beer
:abv_beer rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :has_abv ;
                                owl:someValuesFrom xsd:float
                              ] ;
          rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#amber_ale
:amber_ale rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :has_substyle ;
                                 owl:hasValue :Cream_Ale
                               ] ;
           rdfs:subClassOf :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#amber_colored
:amber_colored rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :has_srm ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:nonNegativeInteger ;
                                                          owl:withRestrictions ( [ xsd:minExclusive "6"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                                 [ xsd:maxInclusive "9"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#amber_lager
:amber_lager rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:unionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_substyle ;
                                                   owl:hasValue :American_Lager
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_substyle ;
                                                   owl:hasValue :International_Amber_Lager
                                                 ]
                                               )
                                 ] ;
             rdfs:subClassOf :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#balanced
:balanced rdf:type owl:Class ;
          rdfs:subClassOf :flavour .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#base_malts
:base_malts rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:oneOf ( :Munich_malt
                                              :Pale_malt
                                              :Pils_malt
                                              :Vienna_malt
                                            )
                                ] ;
            rdfs:subClassOf :malt ;
            owl:disjointWith :special_malts .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#beer
:beer rdf:type owl:Class ;
      owl:equivalentClass [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_ingredient ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :malt
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_ingredient ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :water
                                                 ]
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :has_ingredient ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass :yeast
                                                 ]
                                               ) ;
                            rdf:type owl:Class
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :has_ingredient ;
                            owl:someValuesFrom :other_ingredient
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :created_by ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :beer_producer
                          ] .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#beer_features
:beer_features rdf:type owl:Class .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#beer_producer
:beer_producer rdf:type owl:Class .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#belgian_beer
:belgian_beer rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:unionOf ( [ owl:intersectionOf ( :beer
                                                                         [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :located_in ;
                                                                           owl:hasValue :Belgium
                                                                         ]
                                                                       ) ;
                                                    rdf:type owl:Class
                                                  ]
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :created_by ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass :belgian_brewery
                                                  ]
                                                )
                                  ] ;
              rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#belgian_brewery
:belgian_brewery rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:unionOf ( [ owl:intersectionOf ( :brewery
                                                                            [ rdf:type owl:Restriction ;
                                                                              owl:onProperty :located_in ;
                                                                              owl:hasValue :Belgium
                                                                            ]
                                                                          ) ;
                                                       rdf:type owl:Class
                                                     ]
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :creates ;
                                                       owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                       owl:onClass :belgian_beer
                                                     ]
                                                   )
                                     ] ;
                 rdfs:subClassOf :brewery .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#bitter
:bitter rdf:type owl:Class ;
        rdfs:subClassOf :flavour .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#black_colored
:black_colored rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :has_srm ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:nonNegativeInteger ;
                                                          owl:withRestrictions ( [ xsd:minExclusive "30"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                                 [ xsd:maxInclusive "50"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#blended_beer
:blended_beer rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :blended_with ;
                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass :beer
                                  ] ;
              rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#bock
:bock rdf:type owl:Class ;
      owl:equivalentClass [ rdf:type owl:Restriction ;
                            owl:onProperty :has_substyle ;
                            owl:hasValue :Helles_Bock
                          ] ;
      rdfs:subClassOf :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#brewery
:brewery rdf:type owl:Class ;
         owl:equivalentClass [ rdf:type owl:Restriction ;
                               owl:onProperty :produces_hl ;
                               owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                    owl:onDatatype xsd:nonNegativeInteger ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "0"^^xsd:nonNegativeInteger
                                                                           ]
                                                                           [ xsd:maxExclusive "10000"^^xsd:nonNegativeInteger
                                                                           ]
                                                                         )
                                                  ]
                             ] ;
         rdfs:subClassOf :beer_producer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#brown_ale
:brown_ale rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty :has_substyle ;
                                 owl:hasValue :British_Brown_Ale
                               ] ;
           rdfs:subClassOf :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#brown_colored
:brown_colored rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :has_srm ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:nonNegativeInteger ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "19"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                                 [ xsd:maxInclusive "22"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#collab_beer
:collab_beer rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty :created_by ;
                                   owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
                                   owl:onClass :beer_producer
                                 ] ;
             rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#colored_beer
:colored_beer rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :has_srm ;
                                    owl:someValuesFrom xsd:nonNegativeInteger
                                  ] ;
              rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#container
:container rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( :Bottle
                                             :Can
                                             :Keg
                                           )
                               ] .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#copper_colored
:copper_colored rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Restriction ;
                                      owl:onProperty :has_srm ;
                                      owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                           owl:onDatatype xsd:nonNegativeInteger ;
                                                           owl:withRestrictions ( [ xsd:minExclusive "14"^^xsd:nonNegativeInteger
                                                                                  ]
                                                                                  [ xsd:maxExclusive "17"^^xsd:nonNegativeInteger
                                                                                  ]
                                                                                )
                                                         ]
                                    ] ;
                rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#craft_beer
:craft_beer rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Class ;
                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                  owl:onProperty :created_by ;
                                                  owl:someValuesFrom :brewery
                                                ]
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :created_by ;
                                                  owl:someValuesFrom :homebrewer
                                                ]
                                              )
                                ] ;
            rdfs:subClassOf :beer ;
            owl:disjointWith :industrial_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#dark_brown_colored
:dark_brown_colored rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :has_srm ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:nonNegativeInteger ;
                                                               owl:withRestrictions ( [ xsd:minExclusive "22"^^xsd:nonNegativeInteger
                                                                                      ]
                                                                                      [ xsd:maxInclusive "30"^^xsd:nonNegativeInteger
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#dark_lager
:dark_lager rdf:type owl:Class ;
            owl:equivalentClass [ rdf:type owl:Restriction ;
                                  owl:onProperty :has_substyle ;
                                  owl:hasValue :International_Dark_Lager
                                ] ;
            rdfs:subClassOf :style_family_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#deep_amber_colored
:deep_amber_colored rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :has_srm ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:nonNegativeInteger ;
                                                               owl:withRestrictions ( [ xsd:minInclusive "10"^^xsd:nonNegativeInteger
                                                                                      ]
                                                                                      [ xsd:maxInclusive "14"^^xsd:nonNegativeInteger
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#deep_copper_colored
:deep_copper_colored rdf:type owl:Class ;
                     owl:equivalentClass [ rdf:type owl:Restriction ;
                                           owl:onProperty :has_srm ;
                                           owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                owl:onDatatype xsd:nonNegativeInteger ;
                                                                owl:withRestrictions ( [ xsd:minInclusive "17"^^xsd:nonNegativeInteger
                                                                                       ]
                                                                                       [ xsd:maxInclusive "18"^^xsd:nonNegativeInteger
                                                                                       ]
                                                                                     )
                                                              ]
                                         ] ;
                     rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#era
:era rdf:type owl:Class ;
     owl:equivalentClass [ rdf:type owl:Class ;
                           owl:oneOf ( :Historical
                                       :Modern
                                       :Traditional
                                     )
                         ] ;
     rdfs:subClassOf :beer_features .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#fermentation
:fermentation rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:oneOf ( :Bottom_Fermented
                                                :Top_Fermented
                                                :Wild_Fermented
                                              )
                                  ] ;
              rdfs:subClassOf :beer_features .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#flavour
:flavour rdf:type owl:Class ;
         rdfs:subClassOf :beer_features .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#fruit
:fruit rdf:type owl:Class ;
       rdfs:subClassOf :other_ingredient .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#fruity
:fruity rdf:type owl:Class ;
        rdfs:subClassOf :flavour .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#gold_colored
:gold_colored rdf:type owl:Class ;
              owl:equivalentClass [ rdf:type owl:Restriction ;
                                    owl:onProperty :has_srm ;
                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                         owl:onDatatype xsd:nonNegativeInteger ;
                                                         owl:withRestrictions ( [ xsd:minInclusive "5"^^xsd:nonNegativeInteger
                                                                                ]
                                                                                [ xsd:maxInclusive "6"^^xsd:nonNegativeInteger
                                                                                ]
                                                                              )
                                                       ]
                                  ] ;
              rdfs:subClassOf :colored_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#high_ibu_beer
:high_ibu_beer rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty :has_ibu ;
                                     owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                          owl:onDatatype xsd:nonNegativeInteger ;
                                                          owl:withRestrictions ( [ xsd:minInclusive "60"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                                 [ xsd:maxInclusive "120"^^xsd:nonNegativeInteger
                                                                                 ]
                                                                               )
                                                        ]
                                   ] ;
               rdfs:subClassOf :ibu_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#high_strength_beer
:high_strength_beer rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Restriction ;
                                          owl:onProperty :has_abv ;
                                          owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                               owl:onDatatype xsd:float ;
                                                               owl:withRestrictions ( [ xsd:minInclusive "6.0"^^xsd:float
                                                                                      ]
                                                                                      [ xsd:maxExclusive "9.0"^^xsd:float
                                                                                      ]
                                                                                    )
                                                             ]
                                        ] ;
                    rdfs:subClassOf :abv_beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#historical_beer
:historical_beer rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :has_era ;
                                       owl:hasValue :Historical
                                     ] ;
                 rdfs:subClassOf :beer ;
                 owl:disjointWith [ owl:intersectionOf ( :modern_beer
                                                         :traditional_beer
                                                       ) ;
                                    rdf:type owl:Class
                                  ] .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#homebrewer
:homebrewer rdf:type owl:Class ;
            rdfs:subClassOf :beer_producer ,
                            :person .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#hop
:hop rdf:type owl:Class ;
     rdfs:subClassOf :other_ingredient .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#hoppy
:hoppy rdf:type owl:Class ;
       rdfs:subClassOf :flavour .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#ibu_beer
:ibu_beer rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :has_ibu ;
                                owl:someValuesFrom xsd:nonNegativeInteger
                              ] ;
          rdfs:subClassOf :beer .


###  http://www.semanticweb.org/tom/ontologies/2021/0/untitled-ontology-5#industrial_beer
:industrial_beer rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Restriction ;
                                       owl:onProperty :created_by ;
                                       owl:someValuesFrom :industry
                                     ] ;
